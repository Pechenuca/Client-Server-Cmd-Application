<?import javafx.scene.layout.StackPane?>

<StackPane prefHeight="750.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ImageView fitHeight="885.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../media/menhera.jpg" />
            </image>
        </ImageView>
        <Label styleClass="splash-title" text="%splash.title" wrapText="true">
            <padding>
                <Insets bottom="20.0" left="15.0" right="15.0" top="20.0" />
            </padding>
        </Label>
    </children>
</StackPane>